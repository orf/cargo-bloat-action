{
  "name": "cargo-bloat-action",
  "version": "0.0.0",
  "private": true,
  "description": "Track bloat in your Rust projects",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "pack": "ncc build",
    "build-release": "npm run build && npm run pack",
    "all": "npm run build && npm run pack",
    "add-release": "git add dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orf/cargo-bloat-action.git"
  },
  "pre-commit": [
    "build-release",
    "add-release"
  ],
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "orf",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^3.0.0",
    "@actions/io": "^1.0.2",
    "axios": "^0.19.2",
    "diff": "^4.0.2",
    "filesize": "^6.1.0",
    "lodash": "^4.17.15",
    "text-table": "^0.2.0",
    "treeify": "^1.1.0"
  },
  "devDependencies": {
    "@types/lodash.set": "^4.3.6",
    "pre-commit": "^1.2.2",
    "@types/diff": "^4.0.2",
    "@types/handlebars-helpers": "^0.5.2",
    "@types/text-table": "^0.2.1",
    "@types/treeify": "^1.0.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@zeit/ncc": "^0.22.3",
    "js-yaml": "^3.14.0",
    "typescript": "^3.9.5"
  }
}
